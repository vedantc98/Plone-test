<configure
  xmlns="http://namespaces.zope.org/zope"
  xmlns:i18n="http://namespaces.zope.org/i18n"
  xmlns:browser="http://namespaces.zope.org/browser"
  i18n_domain="collective.protected">

  <browser:page
    for="*"
    name="test-unprotected"
    class="plone.protect.testing.TestUnprotectedView"
    permission="zope2.View"
  />

  <browser:page
    for="*"
    name="test-safetowrite"
    class="plone.protect.testing.TestSafeToWriteObject"
    permission="zope2.View"
  />

</configure>
